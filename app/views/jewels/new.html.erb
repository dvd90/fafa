<h1>New_Jewel</h1>
<!-- app/views/articles/_form.html.erb -->
<%= simple_form_for(@jewel) do |f| %>
  <%= f.input :name %>
  <%= f.input :description %>
  <%= f.input :price %>
  <%= f.input :photo %>
  <%= f.input :photo_cache, as: :hidden %>
  <%= f.input :available, as: :boolean %>
  <%= f.submit "add a jewel", class: "btn btn-primary"%>
<% end %>
